# Custom Aliases
alias open='xdg-open'

alias caps-esc='setxkbmap -option caps:escape'
alias caps-super='setxkbmap -option caps:super'
alias caps-reset='setxkbmap -option'

alias ssss='sassc --style compact ./sass/style.scss ./style.css'

alias clip='xclip -selection clipboard'

alias reddit='tuir'

alias gcal='gcalcli'
alias agenda='gcalcli agenda'
alias today='gcalcli agenda "12:00am" "11:59pm" --details all'
alias week='gcalcli calw'
alias month='gcalcli calm'

alias action-items='trello show-cards --board ToDo --list "Action Items"'

alias mapscii='telnet mapscii.me'
